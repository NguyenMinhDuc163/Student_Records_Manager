<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ForgotPassController">
   <children>
      <Label alignment="CENTER" layoutX="210.0" layoutY="44.0" prefHeight="39.0" prefWidth="197.0" text="Forgot Your Password">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="132.0" layoutY="100.0" text="Enter your email address to retrieve your password" />
      <TextField fx:id="username" layoutX="208.0" layoutY="169.0" prefHeight="31.0" prefWidth="229.0" />
      <TextField fx:id="password" layoutX="208.0" layoutY="244.0" prefHeight="31.0" prefWidth="229.0" />
      <Label alignment="CENTER" layoutX="97.0" layoutY="169.0" text="UserName">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="109.0" layoutY="247.0" prefHeight="21.0" prefWidth="55.0" text="Email">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Button layoutX="375.0" layoutY="317.0" mnemonicParsing="false" onAction="#setButtonSubmit" text="Submit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button layoutX="210.0" layoutY="317.0" mnemonicParsing="false" onAction="#setButtonCancel" text="Cancel">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
